{
  "manifest_version": 2,
  "name": "bubblevote",
  "description": "bubble vote",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*","https://*/*","tabs"
  ],
  "background":{
    "page": "background.html"
  }
}
